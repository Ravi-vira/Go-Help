  .logo {
    display: flex;
    align-items: center;
    font-size: 1.5rem;
    font-weight: bold;
    color: #333;
    text-decoration: none;
  }

  .logo img {
    height: 85px;
    margin-right: 10px;
  }

  .nav-links {
    display: flex;
    gap: 3rem;
    list-style: none;
    margin-right: 50px;
  }

  .nav-links a {
    text-decoration: none;
    color: #333;
    font-size: 1rem;
  }

  .sign-up-btn {
    background-color: #ff8c00;
    color: white;
    padding: 0.5rem 1.5rem;
    border-radius: 25px;
    text-decoration: none;
    font-weight: bold;
    margin-left: 15px;
  }

  /* Hero Section Styles */
  .hero {
    display: flex;
    padding: 4rem 5%;
    position: relative;
    overflow: hidden;
    min-height: calc(100vh - 80px);
  }

  .hero-content {
    flex: 1;
    max-width: 600px;
    z-index: 3;
    position: absolute;
    top: 133px;
  }

  .hero-image {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 2;
  }

  .hero-image img {
    position: absolute;
    max-width: 100%;
    height: 700px;
    width: 900px;
    right: 35px;
    -webkit-transform: scaleX(-1);
    transform: scaleX(-1);
  }

  .hero h1 {
    font-size: 3rem;
    margin-bottom: 1rem;
    line-height: 1.2;
  }

  .hero p {
    font-size: 1rem;
    color: #666;
    margin-bottom: 2rem;
    line-height: 1.6;
  }

  .request-btn {
    display: inline-block;
    background-color: #ff8c00;
    color: white;
    padding: 1rem 2rem;
    border-radius: 25px;
    text-decoration: none;
    font-weight: bold;
    font-size: 1.1rem;
  }

  /* Wave Effect */
  .wave-container {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 200px;
    z-index: 1;
    overflow: hidden;
  }

  .wave {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(transparent 50%, #ff8c00 50%);
    opacity: 0.3;
  }

  .wave:nth-child(1) {
    animation: wave 8s linear infinite;
    opacity: 0.3;
  }

  .wave:nth-child(2) {
    animation: wave 10s linear infinite;
    opacity: 0.2;
  }

  .wave:nth-child(3) {
    animation: wave 12s linear infinite;
    opacity: 0.1;
  }

  @keyframes wave {
    0% {
      transform: translateX(0) scaleY(1);
    }

    50% {
      transform: translateX(-25%) scaleY(0.9);
    }

    100% {
      transform: translateX(-50%) scaleY(1);
    }
  }

  /* Responsive Design */
  @media (max-width: 1024px) {
    .hero h1 {
      font-size: 2.5rem;
    }

    .nav-links {
      gap: 1rem;
    }
  }

  @media (max-width: 768px) {
